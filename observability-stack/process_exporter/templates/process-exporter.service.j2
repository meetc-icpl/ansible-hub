[Unit]
Description=Process Exporter
#Documentation=https://github.com/ncabatoff/process-exporter
After=network.target

[Service]
User=root
Group=root
Type=simple
ExecStart=/usr/local/bin/process-exporter --config.path /usr/bin/process-exporter/config.yml --web.listen-address=:9256
Restart=always
RestartSec=1
StartLimitInterval=0

[Install]
WantedBy=multi-user.target
